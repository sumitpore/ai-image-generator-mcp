{
  "mcpServers": {
    "fal-imagekit": {
      "command": "node",
      "args": [
        "/absolute/path/to/fal-imagekit-mcp/dist/index.js"
      ],
      "env": {
        "FAL_KEY": "your_fal_api_key_here",
        "IMAGEKIT_PUBLIC_KEY": "your_imagekit_public_key_here",
        "IMAGEKIT_PRIVATE_KEY": "your_imagekit_private_key_here"
      }
    }
  }
}

